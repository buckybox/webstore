- raise "#{params[:webstore_id]} != #{current_webstore.id}" if params[:webstore_id] != current_webstore.id

- cache([request.fullpath, products.map(&:id).hash, products.map(&:updated_at).max]) do
  %noscript
    .alert.alert-error
      = t('no_js')

  .row-fluid
    .span12
      #webstore-items= render partial: 'product', collection: products
